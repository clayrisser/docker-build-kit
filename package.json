{
  "name": "dbk",
  "version": "0.2.1",
  "description": "Tooling for building and testing docker images",
  "homepage": "https://github.com/jamrizzi/dbk",
  "author": {
    "name": "Jam Risser",
    "email": "jam@jamrizzi.com",
    "url": "https://jam.jamrizzi.com"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run clean && npm run lint && babel src -d lib",
    "clean": "rm -rf lib",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "prepublish": "npm run build",
    "start": "npm run lint && babel-node src -r demo"
  },
  "dependencies": {
    "boom": "^7.2.0",
    "commander": "^2.14.1",
    "easy-joi": "0.0.2",
    "easycp": "0.0.2",
    "joi": "^13.1.2",
    "js-yaml": "^3.10.0",
    "lodash": "^4.17.5",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.9.0"
  },
  "license": "MIT",
  "repository": "https://github.com/jamrizzi/dbk",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "4"
          }
        }
      ],
      "stage-2"
    ]
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "eslintIgnore": [
    "lib"
  ]
}
