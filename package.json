{
  "name": "dbk",
  "version": "0.1.2",
  "description": "Tooling for building and testing docker images",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "start": "npm run build && node ./lib -h",
    "build": "node ./node_modules/jnwb/lib/bin/nwb build-web-module",
    "clean": "node ./node_modules/jnwb/lib/bin/nwb clean-module",
    "test": "node ./node_modules/jnwb/lib/bin/nwb test",
    "test:coverage": "node ./node_modules/jnwb/lib/bin/nwb test --coverage",
    "test:watch": "node ./node_modules/jnwb/lib/bin/nwb test --server"
  },
  "dependencies": {
    "commander": "^2.13.0",
    "js-yaml": "^3.10.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "jnwb": "^0.1.5"
  },
